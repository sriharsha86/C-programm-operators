#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter two numbers :\n");
    scanf("%d\n%d", &a, &b);
    printf("the arthematic operators result is\n for\n a+b = %d\n a-b= %d\n a*b= %d\n a/b= %d", a+b, a-b, a*b, a/b);
    printf("\nthe relational operators result is\n for\n a > b  %d\n a < b %d\n a = b  %d\n ", a>b, a<b, a==b);
    printf("\nthe logical operators result is\n for\n a AND b  %d\n a OR b %d\n NOT(a==b)  %d\n ", a&&b, a||b, !(a==b));
    printf("The increament operators for \na++ %d\n ++a %d\n b++ %d\n ++b %d\n", a++, ++a, b++, ++b);
    printf("The increament operators for \na-- %d\n --a %d \n b-- %d \n --b %d\n", a--, --a, b--, --b);
    printf("\nBitwise AND operator result is %d\n", a&b);
    printf("Bitwise OR operator result is %d\n", a|b);
    printf("Bitwise NOT operator result is %d\n", a^b);

    return 0;
}
